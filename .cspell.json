{
  "version": "0.2",
  "words": [
    "codeql",
    "devdeps",
    "FOUC",
    "lighttpd",
    "mkdir",
    "modernizr",
    "robotstxt",
    "vuejs",
    "cpina"
  ],
  "language": "en,en-GB,en-US",
  "allowCompoundWords": true,
  "files": ["**/*.md", "**/*.html", "**/*.js", "**/*.css"],
  "ignoreRegExpList": ["\\_\\w+", "\\#\\w+"],
  "ignorePaths": [
    ".cspell.json",
    "node_modules",
    "dist",
    "build",
    ".git",
    ".vscode"
  ],
  "useGitignore": true
}
